<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#424242"
    tools:context=".dac_status">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_marginBottom="600dp"
        android:text="@string/audio_dac_button"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.869" />

    <android.support.v7.widget.CardView
        android:id="@+id/cv_main"
        android:layout_width="410dp"
        android:layout_height="270dp"
        app:cardCornerRadius="12sp"
        app:cardElevation="0.4sp"
        app:cardUseCompatPadding="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="270dp">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="24dp"
                android:layout_marginLeft="100dp"
                android:text="@string/audio_dac_status"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/uhqa_title"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="80dp"
                android:layout_marginLeft="24dp"
                android:text="@string/audio_dac_uhqa_status"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textview4"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="80dp"
                android:layout_marginLeft="145dp"
                android:text=":"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/uhqa_status"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="80dp"
                android:layout_marginLeft="160dp"
                android:text="@string/empty"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/hph_title"
                android:layout_width="71dp"
                android:layout_height="wrap_content"
                android:layout_marginVertical="120dp"
                android:layout_marginLeft="24dp"
                android:text="@string/audio_dac_hph_status"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/hph_status"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="120dp"
                android:layout_marginLeft="160dp"
                android:text="@string/empty"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textview5"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="120dp"
                android:layout_marginLeft="145dp"
                android:text=":"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/amp_title"
                android:layout_width="71dp"
                android:layout_height="wrap_content"
                android:layout_marginVertical="160dp"
                android:layout_marginLeft="24dp"
                android:text="@string/audio_dac_amp_status"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/amp_status"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="160dp"
                android:layout_marginLeft="160dp"
                android:text="@string/empty"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textview6"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="160dp"
                android:layout_marginLeft="145dp"
                android:text=":"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/impedance_title"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:layout_marginVertical="200dp"
                android:layout_marginLeft="24dp"
                android:text="@string/audio_dac_impedance_status"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/impedance_status"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="200dp"
                android:layout_marginLeft="160dp"
                android:text="@string/empty"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textview7"
                android:layout_width="wrap_content"
                android:layout_height="31dp"
                android:layout_marginVertical="200dp"
                android:layout_marginLeft="145dp"
                android:text=":"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#000000" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>

</android.support.constraint.ConstraintLayout>
